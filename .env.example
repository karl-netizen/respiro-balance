
# Supabase Configuration (Required)
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Application Configuration (Required)
VITE_APP_ENV=production
VITE_DEMO_MODE=false
VITE_SITE_URL=https://your-site-url.com

# Stripe Configuration (Edge Functions Only - Set in Supabase Dashboard)
# These are managed as Supabase secrets, not environment variables
# STRIPE_SECRET_KEY=your-stripe-secret-key
# STRIPE_WEBHOOK_SIGNING_SECRET=your-stripe-webhook-signing-secret
# STRIPE_PREMIUM_PRICE_ID=your-premium-price-id
# STRIPE_TEAM_PRICE_ID=your-team-price-id

# Feature Flags (Optional)
VITE_ENABLE_BIOMETRIC_INTEGRATION=true
VITE_ENABLE_OFFLINE_MODE=true

# Security Headers (Optional)
VITE_CSP_ENABLED=true
